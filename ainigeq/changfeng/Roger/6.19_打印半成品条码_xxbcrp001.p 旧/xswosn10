SYSVAR(18)=0
SYSVAR(35)=1
LTS& OFF
INPUT OFF
FORMAT INPUT CHR$(2),CHR$(3),CHR$(13)
LAYOUT INPUT "tmp:LBLSOFT.LAY"
NASC -2:AN 7
CLIP OFF
CLIP BARCODE OFF
XORMODE OFF
MAG 1,1
BF "Monospace 821 BT",7,0,0,1,1 ON
PP 73,114:DIR 1:BT "CODE128":BM 2:BH 88:PB "@b"
NASC -2
AN 7
DIR 1:PP 488,25:FT "Swiss 721 BT",8,0,104:NI:PT "@q"
PP 552,24:FT "Swiss 721 BT",8,0,109:NI:PT "@n"
LAYOUT END
LAYOUT RUN "tmp:LBLSOFT.LAY"
INPUT ON
PF 
PRINT KEY OFF

