ADD TABLE "xxwrd_det"
  AREA "Schema Area"
  LABEL "xxwrd_det"
  DUMP-NAME "xxwrd_de"

ADD FIELD "xxwrd_wonbr" OF "xxwrd_det" AS character 
  FORMAT "X(18)"
  INITIAL ""
  LABEL "工单号"
  POSITION 2
  SQL-WIDTH 36
  ORDER 10

ADD FIELD "xxwrd_wolot" OF "xxwrd_det" AS character 
  FORMAT "X(8)"
  INITIAL ""
  LABEL "工单标志"
  POSITION 3
  SQL-WIDTH 16
  ORDER 20

ADD FIELD "xxwrd_part" OF "xxwrd_det" AS character 
  FORMAT "X(18)"
  INITIAL ""
  LABEL "项目号"
  POSITION 4
  SQL-WIDTH 36
  ORDER 30

ADD FIELD "xxwrd_op" OF "xxwrd_det" AS integer 
  FORMAT ">>>>>9"
  INITIAL "0"
  LABEL "工序"
  POSITION 5
  SQL-WIDTH 4
  ORDER 40

ADD FIELD "xxwrd_wrnbr" OF "xxwrd_det" AS integer 
  DESCRIPTION "SFC工单首次刷读时创建的关联号码,用作同工单号不同加工批次的区分标志"
  FORMAT ">>>>>9"
  INITIAL "0"
  LABEL "SFC关联号"
  POSITION 6
  SQL-WIDTH 4
  ORDER 50

ADD FIELD "xxwrd_opfinish" OF "xxwrd_det" AS logical 
  DESCRIPTION "标识此工序是否加工完成,仅对最大的工单ID生效"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "工序完成"
  POSITION 7
  SQL-WIDTH 1
  ORDER 60

ADD FIELD "xxwrd_status" OF "xxwrd_det" AS character 
  DESCRIPTION "工序的类型,限制值:空-正常工序,N-SFC加入的工序,D-SFC删除的工序"
  FORMAT "X(1)"
  INITIAL ""
  LABEL "工序类型"
  POSITION 8
  SQL-WIDTH 2
  ORDER 70

ADD FIELD "xxwrd_qty_ord" OF "xxwrd_det" AS decimal 
  FORMAT "->,>>>,>>9.9<<<<<<<<"
  INITIAL "0"
  LABEL "订购数"
  POSITION 9
  SQL-WIDTH 25
  DECIMALS 10
  ORDER 80

ADD FIELD "xxwrd_qty_comp" OF "xxwrd_det" AS decimal 
  FORMAT "->,>>>,>>9.9<<<<<<<<"
  INITIAL "0"
  LABEL "合格数"
  POSITION 10
  SQL-WIDTH 25
  DECIMALS 10
  ORDER 90

ADD FIELD "xxwrd_qty_rejct" OF "xxwrd_det" AS decimal 
  FORMAT "->,>>>,>>9.9<<<<<<<<"
  INITIAL "0"
  LABEL "报废数"
  POSITION 11
  SQL-WIDTH 25
  DECIMALS 10
  ORDER 100

ADD FIELD "xxwrd_qty_rework" OF "xxwrd_det" AS decimal 
  DESCRIPTION "需返修的不良品数量,多次返工,则累计每次的件次"
  FORMAT "->,>>>,>>9.9<<<<<<<<"
  INITIAL "0"
  LABEL "返工开始数"
  POSITION 12
  SQL-WIDTH 25
  DECIMALS 10
  ORDER 110

ADD FIELD "xxwrd_qty_return" OF "xxwrd_det" AS decimal 
  DESCRIPTION "返修实际完成的数量,包含返修后合格/报废/再返工的所有数量"
  FORMAT "->,>>>,>>9.9<<<<<<<<"
  INITIAL "0"
  LABEL "返工完成数"
  POSITION 13
  SQL-WIDTH 25
  DECIMALS 10
  ORDER 120

ADD FIELD "xxwrd_time_setup" OF "xxwrd_det" AS integer 
  FORMAT ">>>>>>>9"
  INITIAL "0"
  LABEL "设置时间"
  POSITION 14
  SQL-WIDTH 4
  ORDER 130

ADD FIELD "xxwrd_time_run" OF "xxwrd_det" AS integer 
  FORMAT ">>>>>>>9"
  INITIAL "0"
  LABEL "运行时间"
  POSITION 15
  SQL-WIDTH 4
  ORDER 140

ADD FIELD "xxwrd_wc" OF "xxwrd_det" AS character 
  FORMAT "x(8)"
  INITIAL ""
  LABEL "机器"
  POSITION 17
  SQL-WIDTH 16
  ORDER 160

ADD FIELD "xxwrd_qty_bom" OF "xxwrd_det" AS decimal 
  DESCRIPTION "biomet的默认值都是1,不为1的情况相关反馈程式需重新测试"
  FORMAT "->>,>>9.99"
  INITIAL "1"
  LABEL "单位用量"
  POSITION 18
  SQL-WIDTH 17
  DECIMALS 10
  ORDER 170

ADD FIELD "xxwrd_lastop" OF "xxwrd_det" AS logical 
  DESCRIPTION "标识是否同工单ID的最后一道工序,作为SFC反馈后是否更新MFGPRO的判断依据"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "最后工序"
  POSITION 19
  SQL-WIDTH 1
  ORDER 180

ADD FIELD "xxwrd_lastwo" OF "xxwrd_det" AS logical 
  DESCRIPTION "标识是否整套工单的最大一个工单ID,也就是发原材料的工单ID"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "最后工单ID"
  POSITION 20
  SQL-WIDTH 1
  ORDER 190

ADD FIELD "xxwrd_issok" OF "xxwrd_det" AS logical 
  DESCRIPTION "标识原材料的发料数量是否正确,退料全部完成时,手工赋值为yes,仅对最大的工单ID生效"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "领退料完成"
  POSITION 21
  SQL-WIDTH 1
  ORDER 200

ADD FIELD "xxwrd_inv_lot" OF "xxwrd_det" AS character 
  DESCRIPTION "取自整套工单的最小的工单标志,即成品工单标志的产品批次 "
  FORMAT "x(18)"
  INITIAL ""
  LABEL "产品批号"
  POSITION 22
  SQL-WIDTH 36
  ORDER 210

ADD FIELD "xxwrd_close" OF "xxwrd_det" AS logical 
  DESCRIPTION "标识是否已经更新MFGPRO"
  FORMAT "yes/no"
  INITIAL "no"
  LABEL "工序已结"
  POSITION 23
  SQL-WIDTH 1
  ORDER 220

ADD FIELD "xxwrd_opname" OF "xxwrd_det" AS character 
