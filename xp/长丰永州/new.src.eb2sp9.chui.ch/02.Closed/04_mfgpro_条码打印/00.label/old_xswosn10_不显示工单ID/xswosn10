SYSVAR(18)=0
SYSVAR(35)=1
LTS& OFF
INPUT OFF
FORMAT INPUT CHR$(2),CHR$(3),CHR$(13)
LAYOUT INPUT "tmp:LBLSOFT.LAY"
NASC -2:AN 7
CLIP OFF
CLIP BARCODE OFF
XORMODE OFF
MAG 1,1
BF "Monospace 821 BT",7,0,0,1,1 ON
PP 75,115:DIR 1:BT "CODE128":BM 2:BH 88:PB "@b"
NASC -2
AN 7
DIR 1:PP 576,26:FT "Swiss 721 BT",7,0,100:NI:PT "@q"
LAYOUT END
LAYOUT RUN "tmp:LBLSOFT.LAY"
INPUT ON
PF
PRINT KEY OFF

